<root>
  <item>
    <path>civicrm/admin/activityical/settings</path>
    <page_callback>CRM_Activityical_Form_Settings</page_callback>
    <title>CiviCRM Activities iCalendar Feed: Settings</title>
    <access_arguments>administer CiviCRM</access_arguments>
  </item>
  <item>
    <path>civicrm/activityical/details</path>
    <page_callback>CRM_Activityical_Form_Details</page_callback>
    <title>CiviCRM Activities iCalendar Feed: Details</title>
    <access_callback>_activityical_check_permission</access_callback>
    <access_arguments>manageFeedDetails</access_arguments>
  </item>
  <item>
    <path>civicrm/activityical/feed</path>
    <page_callback>CRM_Activityical_Page_Feed</page_callback>
    <title>Feed</title>
    <access_callback>_activityical_check_permission</access_callback>
    <access_arguments>viewFeed</access_arguments>
  </item>
</root>
